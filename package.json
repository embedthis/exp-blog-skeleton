{
    "name": "exp-blog-skeleton",
    "description": "Blog Skeleton for Expansive",
    "version": "0.1.0",
    "keywords": [
        "expansive-plugin",
        "blog"
    ],
    "dependencies": {
        "exp-js": "^0.2",
        "exp-less": "^0.2",
        "exp-css": "^0.2",
        "exp-reload": "^0.2",
        "exp-blog": "^0.2",
        "exp-md": "^0.2"
    },
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-blog-skeleton.git"
    },
    "pak": {
        "blend": {
            "pak": {
                "?mode": "debug",
                "?import": true
            }
        },
        "export": {
            "from": [
                "contents/**",
                "layouts/**",
                "partials/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        },
        "manage": "expansive.json",
        "version": ">=0.10.0"
    }
}
