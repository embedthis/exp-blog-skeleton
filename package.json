{
    "name": "exp-blog-skeleton",
    "description": "Blog Skeleton for Expansive",
    "version": "0.2.0",
    "keywords": [
        "expansive-plugin",
        "blog"
    ],
    "dependencies": {
        "exp-blog": "~0.3",
        "exp-canon": "~0.1",
        "exp-css": "~0.3",
        "exp-js": "~0.3",
        "exp-less": "~0.3",
        "exp-reload": "~0.3",
        "exp-md": "~0.2"
    },
    "devDependencies": {
        "expansive": ">=0.6.0",
        "pak": ">=0.11.3"
    },
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-blog-skeleton.git"
    },
    "pak": {
        "blend": {
            "pak": {
                "?mode": "debug",
                "?import": true
            }
        },
        "export": {
            "from": [
                "contents/**",
                "layouts/**",
                "partials/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        },
        "manage": "expansive.json"
    }
}
