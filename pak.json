{
    "name": "exp-blog-skeleton",
    "description": "Blog Skeleton for Expansive",
    "version": "1.0.0",
    "keywords": [
        "expansive-plugin",
        "blog"
    ],
    "dependencies": {
        "embedthis/exp-blog": "~1",
        "embedthis/exp-canon": "~1",
        "embedthis/exp-css": "~1",
        "embedthis/exp-js": "~1",
        "embedthis/exp-less": "~1",
        "embedthis/exp-reload": "~1",
        "embedthis/exp-markdown": "~1"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "pak": ">=0.11.3"
    },
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-blog-skeleton.git"
    },
    "blend": {
        "?profile": "debug",
        "?import": true
    },
    "export": {
        "from": [
            "contents/**",
            "layouts/**",
            "partials/**"
        ],
        "to": "${TOP}",
        "overwrite": false
    },
    "manage": "expansive.json",
    "origin": "embedthis/exp-blog-skeleton"
}
